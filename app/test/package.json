{
  "name": "test",
  "version": "1.0.0",
  "description": "UI5 Application: test",
  "author": "Ugur Biricik",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "node -e \"const fs = require('fs'); const path = require('path'); ['dist', 'coverage'].forEach(dir => { if (fs.existsSync(dir)) fs.rmSync(dir, { recursive: true, force: true }); });\"",
    "build": "ui5 build preload --clean-dest --config ui5-dist.yaml",
    "build:opt": "ui5 build self-contained --clean-dest --all",
    "start": "ui5 serve --port 8080 -o index.html",
    "start-cdn": "ui5 serve --port 8080 -o index-cdn.html",
    "start:dist": "npm start -- --config ui5-dist.yaml",
    "start:dist-cdn": "npm run start-cdn -- --config ui5-dist.yaml",
    "lint": "eslint webapp",
    "test": "npm run lint",
    "wdi5": "wdio run ./webapp/test/e2e/\\wdio.conf.js",
    "build-local": "ui5 build preload --clean-dest"
  },
  "devDependencies": {
    "@ui5/cli": "^4.0.6",
    "@ui5/middleware-code-coverage": "^2.0.0",
    "@wdio/cli": "^8.45.0",
    "@wdio/local-runner": "^8.45.0",
    "@wdio/mocha-framework": "^8.45.0",
    "@wdio/spec-reporter": "^8.43.0",
    "eslint": "^9.9.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "globals": "^15.9.0",
    "ui5-middleware-livereload": "^3.0.3",
    "ui5-task-zipper": "^3.4.2",
    "wdio-ui5-service": "^2.2.1"
  },
  "main": "webapp/index.html",
  "ui5": {
    "dependencies": [
      "ui5-task-zipper"
    ]
  }
}
